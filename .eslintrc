{
  "extends": ["eslint:recommended","plugin:@typescript-eslint/recommended", "plugin:react/recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
     "react" : {
       "version": "detect"
     }
  },
  "env": {
    "browser": true,
    "es2021": true

  },
  "rules": {
    "no-unused-vars": "warn",
    "react/react-in-jsx-scope": "off"
  },
  "globals": {
    "__IS_DEV__": true
  }
}